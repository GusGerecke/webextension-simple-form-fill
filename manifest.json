{
    "applications": {
        "gecko": {
            "id": "simpleformfill@sblask",
            "strict_min_version": "48.0"
        }
    },
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": "icon.svg",
        "default_title": "Simple Form Fill"
    },
    "description": "Enter text into input fields by choosing items from the context menu or using the optional autocomplete.",
    "homepage_url": "https://addons.mozilla.org/firefox/addon/simple-form-fill/",
    "icons": {
        "128": "icon-128x128.png",
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "48": "icon-48x48.png",
        "64": "icon-64x64.png"
    },
    "manifest_version": 2,
    "name": "Simple Form Fill",
    "options_ui": {
        "open_in_tab": true,
        "page": "options/options.html"
    },
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "storage"
    ],
    "version": "2.0.1"
}
